    },
    "private": true,
    "dependencies": {
        "ansi-colors": "^4.1.3",
        "axios": "^0.27.2",
        "bcrypt": "^5.0.0",
        "busboy": "^1.6.0",
        "cheerio": "^1.0.0-rc.11",
        "cli-progress": "^3.11.1",
        "cors": "^2.8.5",
        "express": "^4.18.1",
        "marked": "^4.0.17",
        "mongoose": "^6.3.6",
        "redis": "^4.2.0",
        "rxjs": "^7.5.6",
        "source-map-support": "^0.5.21",
        "swagger-ui-express": "^4.4.0",
        "tslib": "^2.2.0",
        "typescript": "^4.0.2",
        "typescript-json-schema": "^0.54.0"
    },
    "devDependencies": {
        "@types/bcrypt": "^3.0.0",
        "@types/busboy": "^1.5.0",
        "@types/cheerio": "^0.22.31",
        "@types/cli-progress": "^3.11.0",
        "@types/express": "^4.17.11",
        "@types/node": "^14.10.2",
        "concurrently": "^5.3.0",
        "nodemon": "^2.0.4",
        "@types/axios": "^0.14.0"
    }
}