{
    "name": "workflow-engine-service",
    "version": "1.0.0",
    "main": "build/index.js",
    "scripts": {
        "build": "tsc -b tsconfig.json",
        "start": "tsc -b tsconfig.json; node --trace-warnings build/index.js prod",
